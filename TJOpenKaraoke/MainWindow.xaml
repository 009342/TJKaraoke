<Window x:Name="mainWindow" x:Class="TJOpenKaraoke.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TJOpenKaraoke"
        mc:Ignorable="d"
        Title="MainWindow" Height="441.935" Width="545.161" Loaded="mainWindow_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <ComboBox x:Name="MIDI_PortA" HorizontalAlignment="Stretch" Margin="10,10" VerticalAlignment="Top" Grid.Row="0"/>
        <ComboBox x:Name="MIDI_PortB" HorizontalAlignment="Stretch" Margin="10,10" VerticalAlignment="Top" Grid.Row="1"/>
        <ComboBox x:Name="RegionSelect" HorizontalAlignment="Stretch" Margin="10,10" VerticalAlignment="Top" Grid.Row="2"/>
        <ListBox x:Name="files" HorizontalAlignment="Stretch" Margin="10,10" VerticalAlignment="Top" Grid.Row="3"/>
        <Button x:Name="run" Content="시작" HorizontalAlignment="Stretch" Margin="10,10" VerticalAlignment="Top" Grid.Row="4" Click="run_Click"/>


    </Grid>
</Window>
